<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="TRS" nsURI="https://org.trs" nsPrefix="org.trs">
  <eClassifiers xsi:type="ecore:EClass" name="POI">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="location" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="topicOfInterest" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UserPreferences">
    <eStructuralFeatures xsi:type="ecore:EReference" name="preferredItinerary" eType="#//Itinerary"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Itinerary">
    <eStructuralFeatures xsi:type="ecore:EReference" name="rankedPOIList" eType="#//POI"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OutdoorPOI" eSuperTypes="#//POI">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="meansOfTransportion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IndoorPOI" eSuperTypes="#//POI">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="workingHours" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="capacity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TransportationMode" serializable="false">
    <eLiterals name="BIKE"/>
    <eLiterals name="CAR" value="1"/>
    <eLiterals name="BUS" value="2"/>
    <eLiterals name="TRAIN" value="3"/>
    <eLiterals name="WALK" value="4"/>
  </eClassifiers>
</ecore:EPackage>
